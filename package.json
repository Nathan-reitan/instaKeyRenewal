{
  "name": "instaKeyRenewal",
  "version": "1.0.0",
  "description": "A script designed to automatically renew an Instagram API key.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all --silent --parallel dev:*",
    "dev:client": "webpack-dev-server",
    "dev:server": "nodemon index.js",
    "db:import": "source .env; psql $DATABASE_URL < database/dump.sql",
    "db:export": "source .env; pg_dump -c -O $DATABASE_URL > database/dump.sql",
    "build": "webpack --mode=production",
    "start": "NODE_ENV=production node index.js",
    "listen": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nathan-reitan/instaKeyRenewal.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Nathan-reitan/instaKeyRenewal/issues"
  },
  "homepage": "https://github.com/Nathan-reitan/instaKeyRenewal#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "node-fetch": "^2.6.1",
    "pg": "^8.5.1",
    "session-file-store": "^1.5.0"
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "webpack": "^5.29.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
